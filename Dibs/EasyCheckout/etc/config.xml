<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
    <default>
        <payment>
            <dibs_easy_checkout>
                <active>0</active>
                <model>Dibs\EasyCheckout\Model\Payment\Method\Checkout</model>
                <title>Dibs Easy Checkout</title>
                <payment_action>Authorization</payment_action>
                <solution_type>Mark</solution_type>
                <carrier>dibs_free_shipping</carrier>
                <line_items_enabled>1</line_items_enabled>
                <visible_on_cart>1</visible_on_cart>
                <sort_order>1</sort_order>
                <terms_and_conditions_link_type>direct</terms_and_conditions_link_type>
                <allowed_customer_types>B2C</allowed_customer_types>
            </dibs_easy_checkout>
        </payment>
    </default>
</config>